doctype html
html(lang="en")
  head
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
    meta(name='description', content= 'A nice starting configuration')
    title= "Get Shit Done Kit Starter"
    script(src="https://use.fontawesome.com/df8ec2625f.js")
    block stylesheets
      link(rel="icon" href="img/favicon.ico")
      link(href='https://fonts.googleapis.com/css?family=Grand+Hotel|Open+Sans:400,300' rel='stylesheet' type='text/css')
      // build:css css/app.css
      link(rel="stylesheet" href="styles/pe-icon-7-stroke.css")
      link(rel="stylesheet" href="vendors/bootstrap/css/bootstrap.css")
      link(rel="stylesheet" href="styles/gsdk.css")
      link(rel="stylesheet" href="styles/demo.css")
    block styles
      link(rel="stylesheet" href="assets/css/main.css")
      // endbuild

  body
    block content

    block scripts
      // build:js js/app.js
      script(src="vendors/jquery/js/jquery.js")
      script(src="vendors/jquery-ui/js/jquery-ui.js")
      script(src="vendors/bootstrap/js/bootstrap.js")
      script(src="scripts/gsdk-checkbox.js")
      script(src="scripts/gsdk-radio.js")
      script(src="scripts/gsdk-bootstrapswitch.js")
      script(src="scripts/gsdk-morphing.js")
      script(src="scripts/bootstrap-select.js")
      script(src="scripts/bootstrap-datepicker.js")
      script(src="scripts/chartist.min.js")
      script(src="scripts/jquery.tagsinput.js")
      script(src="https://maps.googleapis.com/maps/api/js")
      script(src="scripts/get-shit-done.js")
    block javascript
      script(src="assets/js/main.js")
    // endbuild

